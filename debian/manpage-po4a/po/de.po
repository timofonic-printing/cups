#. type: TP
#: lpr.man:79
#, no-wrap
msgid "B<-J \">I<name>B<\">"
msgstr "B<-J \">I<Name>B<\">"

#. type: TP
#: lpr.man:81
#, no-wrap
msgid "B<-T \">I<name>B<\">"
