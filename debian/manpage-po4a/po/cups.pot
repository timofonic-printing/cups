#. type: TP
#: cupsd-logs.man:152
#, no-wrap
msgid "D"
msgstr ""

#. type: Plain text
#: cupsd-logs.man:155
msgid "Debugging message (LogLevel debug)"
msgstr ""

#. type: TP
#: cupsd-logs.man:155
#, no-wrap
msgid "d"
msgstr ""

#. type: Plain text
#: cupsd-logs.man:158
msgid "Detailed debugging message (LogLevel debug2)"
msgstr ""

#. type: TP
#: cupsd-logs.man:158
#, no-wrap
msgid "E"
msgstr ""

#. type: Plain text
#: cupsd-logs.man:161
msgid "Normal error message (LogLevel error)"
msgstr ""

#. type: TP
#: cupsd-logs.man:161
#, no-wrap
