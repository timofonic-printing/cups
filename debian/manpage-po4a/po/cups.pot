#. type: TP
#: ipptoolfile.man:626
#, no-wrap
msgid "B<$resource>"
msgstr ""

#. type: Plain text
#: ipptoolfile.man:630
msgid "Inserts the resource path from the URI provided to B<ipptool>(8)."
msgstr ""

#. type: TP
#: ipptoolfile.man:630
#, no-wrap
msgid "B<$scheme>"
msgstr ""

#. type: Plain text
#: ipptoolfile.man:634
msgid "Inserts the scheme from the URI provided to B<ipptool>(8)."
msgstr ""

#. type: TP
#: ipptoolfile.man:634
#, no-wrap
msgid "B<$uri>"
msgstr ""

#. type: Plain text
#: ipptoolfile.man:638
msgid "Inserts the URI provided to B<ipptool>(8)."
