#, no-wrap
msgid ""
"B<ssize_t cupsBackChannelRead>(B<char *>I<buffer>, B<size_t >I<bytes>,\n"
"                            B<double >I<timeout>);\n"
msgstr ""

#. type: Plain text
#: filter.man:36
#, no-wrap
msgid ""
"B<cups_sc_status_t cupsSideChannelDoRequest>(B<cups_sc_command_t "
">I<command>,\n"
"                                          B<char *>I<data>, B<int "
"*>I<datalen>,\n"
"                                          B<double >I<timeout>);\n"
