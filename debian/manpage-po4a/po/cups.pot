msgstr ""

#. type: Plain text
#: cupsd.conf.man.in:686
msgid "The path for the specified job."
msgstr ""

#. type: TP
#: cupsd.conf.man.in:686
#, no-wrap
msgid "/printers"
msgstr ""

#. type: Plain text
#: cupsd.conf.man.in:689
msgid "The path for all printers"
msgstr ""

#. type: TP
#: cupsd.conf.man.in:689
